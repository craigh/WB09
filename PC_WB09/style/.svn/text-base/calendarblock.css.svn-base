td.blockmonthon
{
	background-color: white;
	border: 1px solid white;
}
td.blockmonthoff
{
	background-color: white;
	border: 1px solid white;
}
td.blockmonthtoday
{
	background-color: yellow;
	border: 1px solid white;
}
td.blockmonthheader
{
	background-color: white;
	font-size: 8pt;
	font-weight: bold;
}
table.blockmonthheader
{
	background-color: white;
	border: 1px solid #000000;
}
.blockmonthheader a:link
{
	font-size: 8pt;
	font-weight: bold;
	padding-left: 1px;
	padding-right: 1px;
	text-decoration: none;
}
.blockmonthheader a:hover
{
	font-size: 8pt;
	font-weight: bold;
	padding-left: 1px;
	padding-right: 1px;
	text-decoration: none;
}
.blockmonthheader a:active
{
	font-size: 8pt;
	font-weight: bold;
	padding-left: 1px;
	padding-right: 1px;
	text-decoration: none;
}
.blockmonthheader a:visited
{
	font-size: 8pt;
	font-weight: bold;
	padding-left: 1px;
	padding-right: 1px;
	text-decoration: none;
}
td.blockdaynames
{
	background-color: white;
	border: 1px solid white;
	font-size: 8pt;
	font-weight: normal;
	text-decoration: none;
}

table.blockcalcontainer,
td.blockcalcontainer
{
	border: 1px solid #000000;
}
td.blockweeklink
{
	background-color: white;
	border: 1px solid white;
}
.blockweeklink a:link,
.blockweeklink a:hover,
.blockweeklink a:visited,
.blockweeklink a:active
{
	color: black;
	font-size: 7pt;
	font-weight: normal;
	text-decoration: none;
}
a.blockevent-none:link,
