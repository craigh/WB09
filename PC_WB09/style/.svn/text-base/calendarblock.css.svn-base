	font-weight: normal;
	text-decoration: none;
}
li.eventlink,
.eventlink a:link,
.eventlink a:hover,
.eventlink a:visited,
.eventlink a:active
{
	font-size: 10px;
	font-weight: normal;
	padding: 2px;
	text-decoration: none;
}
li.pc_blockdate
{
	font-weight: bold;
	font-size: 1.1em;
}
li.pc_blockevent
{
	margin-left:2em;
	border:none;
	padding:0;
}
div.pc_centerblocksubmitlinks
{
	text-align: center;
	padding: 1em 0 0 0;
	
