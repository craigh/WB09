	font-weight: normal;
	text-decoration: none;
}
a.blockevent-one:link,
a.blockevent-one:hover,
a.blockevent-one:visited,
a.blockevent-one:active
{
	color: #006600;
	font-size: 10px;
	font-weight: normal;
	text-decoration: none;
}
a.blockevent-two:link,
a.blockevent-two:hover,
a.blockevent-two:visited,
a.blockevent-two:active
{
	color: #006666;
	font-size: 10px;
	font-weight: normal;
	text-decoration: none;
}
a.blockevent-three:link,
a.blockevent-three:hover,
a.blockevent-three:visited,
a.blockevent-three:active
{
